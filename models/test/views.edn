#{{:el :context-view
   :id :test/styles-context-view
   :spec {:styles #{{:el :rel
                     :id :dashed-rel
                     :text-color "#000000"
                     :line-color "#000000"
                     :line-style :dashed
                     :tech "async"
                     :legend-text "asynchronous"}
                    {:el :rel
                     :id :dotted-rel
                     :line-style :dotted}
                    {:el :rel
                     :id :bold-rel
                     :line-style :bold
                     :line-color "#007700"
                     :text-color "#000077"}
